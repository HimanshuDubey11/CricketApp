<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.himanshu.com.cricketapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home" />
        <activity android:name=".DataAdapter" />
        <activity android:name=".ScheduleAdapter" />
        <activity android:name=".LiveAdapter" />
        <activity android:name=".Score" />
        <activity android:name=".ScoreDataAdapater" />
        <activity android:name=".PlayerAdapter"></activity>
    </application>

</manifest>